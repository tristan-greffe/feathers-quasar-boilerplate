{
  "name": "feathers-quasar-boilerplate",
  "description": "Feathers Quasar Project Boilerplate",
  "version": "1.0.0",
  "homepage": "https://github.com/tristan-greffe/feathers-quasar-boilerplate",
  "keywords": [
    "Feathers",
    "Quasar",
    "Boilerplate"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tristan-greffe/feathers-quasar-boilerplate.git"
  },
  "author": {
    "name": "Tristan Greffe contributors",
    "url": "https://github.com/tristan-greffe"
  },
  "bugs": {
    "url": "https://github.com/tristan-greffe/feathers-quasar-boilerplate/issues"
  },
  "scripts": {
    "clean": "quasar clean",
    "dev": "cross-env NODE_ENV=development quasar dev",
    "prod": "cd api && npm run prod",
    "build": "quasar build",
    "analyze": "cross-env ANALYZE_BUNDLE=1 npm run build",
    "mocha": "mocha test/**/*.test.mjs --timeout 30000",
    "coverage": "c8 npm run mocha",
    "test:server": "cd api && npm run test",
    "test:client": "cd test && npm run coverage"
  },
  "standard": {
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "vue"
    ],
    "env": [
      "mocha"
    ],
    "globals": [
      "DEV",
      "PROD",
      "localStorage"
    ]
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^5.0.8",
    "@feathersjs/client": "^5.0.8",
    "@feathersjs/feathers": "^5.0.8",
    "@feathersjs/rest-client": "^5.0.8",
    "@feathersjs/socketio-client": "^5.0.8",
    "@quasar/extras": "^1.0.0",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.6.0",
    "browserify-zlib": "^0.2.0",
    "core-js": "^3.6.5",
    "crypto-browserify": "^3.12.0",
    "depcheck": "^1.4.3",
    "https-browserify": "^1.0.0",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.8.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "moment": "^2.29.4",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.4",
    "puppeteer": "^21.0.3",
    "quasar": "^2.6.0",
    "socket.io-client": "^4.6.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.3.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0",
    "vue2-leaflet": "^2.7.1",
    "vue2-leaflet-geosearch": "^1.0.6",
    "weather-icons": "^1.3.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@quasar/app-webpack": "^3.0.0",
    "c8": "^8.0.0",
    "chai": "^4.3.7",
    "chai-lint": "^0.1.1",
    "config": "^3.3.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.15.0",
    "eslint-webpack-plugin": "^4.0.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "superagent": "^8.1.2"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 20.5.0",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
