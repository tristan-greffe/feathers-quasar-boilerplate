{
  "name": "feathers-quasar-boilerplate-api",
  "description": "Feathers Quasar Project Services",
  "version": "1.0.0",
  "homepage": "https://github.com/tristan-greffe/feathers-quasar-boilerplate",
  "keywords": [
    "feathers"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tristan-greffe/feathers-quasar-boilerplate.git"
  },
  "author": {
    "name": "Tristan Greffe contributors",
    "url": "https://github.com/tristan-greffe"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/tristan-greffe/feathers-quasar-boilerplate/issues"
  },
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "release:patch": "npm --no-git-tag-version version patch",
    "release:minor": "npm --no-git-tag-version version minor",
    "release:major": "npm --no-git-tag-version version major",
    "dev": "nodemon --watch src --exec npm run debug",
    "debug": "cross-env NODE_ENV=development node --inspect src/index.js",
    "prod": "cross-env NODE_ENV=production node src/index.js",
    "lint": "standard src/**/*.js test/**/*.js --fix",
    "mocha": "cross-env NODE_CONFIG_DIR=./test/config/ mocha",
    "coverage": "c8 npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "standard": {
    "sourceType": "module",
    "env": [
      "mocha"
    ]
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.5",
    "@feathersjs/authentication": "^5.0.5",
    "@feathersjs/authentication-client": "^5.0.5",
    "@feathersjs/authentication-local": "^5.0.5",
    "@feathersjs/authentication-oauth": "^5.0.5",
    "@feathersjs/configuration": "^5.0.5",
    "@feathersjs/errors": "^5.0.5",
    "@feathersjs/express": "^5.0.5",
    "@feathersjs/feathers": "^5.0.5",
    "@feathersjs/mongodb": "^5.0.5",
    "@feathersjs/schema": "^5.0.5",
    "@feathersjs/socketio": "^5.0.5",
    "@feathersjs/transport-commons": "^5.0.5",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^2.0.0",
    "containerized": "^1.0.2",
    "email-templates": "^2.7.1",
    "feathers-authentication-management": "^4.0.2",
    "feathers-hooks-common": "^8.0.0",
    "feathers-mailer": "^4.1.1",
    "mongodb": "^5.2.0",
    "node-mailjet": "^6.0.4",
    "nodemailer": "^6.9.4",
    "winston": "^3.8.2",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.5",
    "@feathersjs/rest-client": "^5.0.5",
    "axios": "^1.3.4",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  }
}
